<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/arg2TextBox"
        android:layout_width="70dp"
        android:layout_height="41dp"
        android:layout_marginStart="340dp"
        android:layout_marginTop="180dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="85" />

    <Button
        android:id="@+id/button"
        android:layout_width="136dp"
        android:layout_height="38dp"
        android:layout_marginStart="164dp"
        android:layout_marginTop="16dp"
        android:onClick="openDeviceWithCustomValuesOnClick"
        android:text="TWUtil.open"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button2"
        android:layout_width="136dp"
        android:layout_height="57dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:onClick="toggleHVACScreen"
        android:text="Try to toggle HVAC screen"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button3"
        android:layout_width="114dp"
        android:layout_height="38dp"
        android:layout_marginStart="668dp"
        android:layout_marginTop="180dp"
        android:onClick="clearLog"
        android:text="Clear log"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button4"
        android:layout_width="145dp"
        android:layout_height="38dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="72dp"
        android:onClick="attachHandler"
        android:text="Attach handler"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/textView"
        android:layout_width="788dp"
        android:layout_height="234dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="224dp"
        android:duplicateParentState="false"
        android:editable="false"
        android:ems="10"
        android:fadeScrollbars="false"
        android:focusable="false"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:scrollIndicators="top|left|bottom|right"
        android:scrollbarAlwaysDrawVerticalTrack="true"
        android:scrollbarSize="15dp"
        android:scrollbars="horizontal|vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/idList"
        android:layout_width="639dp"
        android:layout_height="41dp"
        android:layout_marginStart="164dp"
        android:layout_marginTop="72dp"
        android:ems="10"
        android:gravity="start|top"
        android:inputType="textMultiLine"
        android:numeric="integer"
        android:singleLine="true"
        android:text="513 515 769 1281 -24804 -25080 -25059 -15872 773 1288 266"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button5"
        android:layout_width="135dp"
        android:layout_height="38dp"
        android:layout_marginStart="308dp"
        android:layout_marginTop="16dp"
        android:onClick="closeDevice"
        android:text="TWUtil.Close"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Switch
        android:id="@+id/rangeModeSwitch"
        android:layout_width="148dp"
        android:layout_height="38dp"
        android:layout_marginStart="452dp"
        android:layout_marginTop="16dp"
        android:checked="false"
        android:clickable="true"
        android:text="Range mode"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:checked="false" />

    <Button
        android:id="@+id/button6"
        android:layout_width="134dp"
        android:layout_height="38dp"
        android:layout_marginStart="424dp"
        android:layout_marginTop="180dp"
        android:onClick="write"
        android:text="TWUtil.Write"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/opTextBox"
        android:layout_width="79dp"
        android:layout_height="41dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="180dp"
        android:editable="true"
        android:ems="10"
        android:inputType="numberDecimal"
        android:singleLine="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="1281" />

    <EditText
        android:id="@+id/arg1TextBox"
        android:layout_width="70dp"
        android:layout_height="41dp"
        android:layout_marginStart="200dp"
        android:layout_marginTop="180dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="170" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="32dp"
        android:layout_height="25dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="180dp"
        android:text="OP:"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="52dp"
        android:layout_height="25dp"
        android:layout_marginStart="141dp"
        android:layout_marginTop="180dp"
        android:text="ARG 1:"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="52dp"
        android:layout_height="25dp"
        android:layout_marginStart="280dp"
        android:layout_marginTop="180dp"
        android:text="ARG 2:"
        android:textAlignment="center"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>